<simulation>
  <archetypes>
    <spec>
      <lib>d3ploy.demand_fac</lib>
      <name>DemandFac</name>
    </spec>
    <spec>
      <lib>agents</lib>
      <name>NullRegion</name>
    </spec>
    <spec>
      <lib>d3ploy.no_inst</lib>
      <name>NOInst</name>
    </spec>
  </archetypes>
  <control>
    <duration>100</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>
  <facility>
    <config>
      <DemandFac>
        <supply_commod>power_rx</supply_commod>
        <demand_commod>fuel_rx</demand_commod>
        <demand_rate_max>2000.</demand_rate_max>
        <demand_rate_min>2000.</demand_rate_min>
        <supply_rate_max>1000.</supply_rate_max>
        <supply_rate_min>1000.</supply_rate_min>
        <demand_ts>16</demand_ts>
        <proto>Reactor</proto>
      </DemandFac>
    </config>
    <name>Reactor</name>
  </facility>
  <facility>
    <config>
      <DemandFac>
        <supply_commod>fuel_en</supply_commod>
        <demand_commod>nat_en</demand_commod>
        <demand_rate_max>200000.</demand_rate_max>
        <demand_rate_min>200000.</demand_rate_min>
        <supply_rate_max>10000.</supply_rate_max>
        <supply_rate_min>10000.</supply_rate_min>
        <proto>Enrichment</proto>
      </DemandFac>
    </config>
    <name>Enrichment</name>
  </facility>
  <facility>
    <config>
      <DemandFac>
        <supply_commod>nat_mine</supply_commod>
        <demand_commod>x</demand_commod>
        <demand_rate_max>0.</demand_rate_max>
        <demand_rate_min>0.</demand_rate_min>
        <supply_rate_max>100000.</supply_rate_max>
        <supply_rate_min>100000.</supply_rate_min>
        <proto>Mine</proto>
      </DemandFac>
    </config>
    <name>Mine</name>
  </facility>
  <region>
    <config>
      <NullRegion>
      </NullRegion>
    </config>
    <institution>
      <config>
        <NOInst>
          <supply_commod>power_rx</supply_commod>
          <demand_commod>power</demand_commod>
          <growth_rate>0.05</growth_rate>
          <prototypes><val>Reactor</val></prototypes>
          <initial_demand>100000.0</initial_demand>
          <deployed>100</deployed>
          <calc_method>ma</calc_method>
        </NOInst>
      </config>
      <initialfacilitylist>
        <entry>
          <number>100</number>
          <prototype>Reactor</prototype>
        </entry>
      </initialfacilitylist>
      <name>ReactorInst</name>
    </institution>
    <institution>
      <config>
        <NOInst>
          <supply_commod>fuel_en</supply_commod>
          <demand_commod>fuel_rx</demand_commod>
          <growth_rate>0.05</growth_rate>
          <prototypes><val>Enrichment</val></prototypes>
          <initial_demand>200000.0</initial_demand>
          <deployed>20</deployed>
          <calc_method>ma</calc_method>
        </NOInst>
      </config>
      <initialfacilitylist>
        <entry>
          <number>20</number>
          <prototype>Enrichment</prototype>
        </entry>
      </initialfacilitylist>
      <name>EnrichInst</name>
    </institution>    
    <institution>
      <config>
        <NOInst>
          <supply_commod>nat_mine</supply_commod>
          <demand_commod>nat_en</demand_commod>
          <growth_rate>0.05</growth_rate>
          <prototypes><val>Mine</val></prototypes>
          <initial_demand>4000000.0</initial_demand>
          <deployed>40</deployed>
          <calc_method>ma</calc_method>
        </NOInst>
      </config>
      <initialfacilitylist>
        <entry>
          <number>40</number>
          <prototype>Mine</prototype>
        </entry>
      </initialfacilitylist>
      <name>MineInst</name>
    </institution>
    <name>SingleRegion</name>
  </region>
</simulation>
