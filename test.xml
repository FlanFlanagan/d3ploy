<simulation>
  <archetypes>
    <spec>
      <lib>d3ploy.DemandFac</lib>
      <name>DemandFac</name>
    </spec>
    <spec>
      <lib>agents</lib>
      <name>NullRegion</name>
    </spec>
    <spec>
      <lib>d3ploy.NOInst</lib>
      <name>NOInst</name>
    </spec>
  </archetypes>
  <control>
    <duration>35</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>
  <facility>
    <config>
      <DemandFac>
        <commodity>bears</commodity>
        <production_rate_max>12</production_rate_max>
        <production_rate_min>8</production_rate_min>
      </DemandFac>
    </config>
    <name>BearStore</name>
  </facility>
  <region>
    <config>
      <NullRegion>
      </NullRegion>
    </config>
    <institution>
      <config>
        <NOInst>
          <growth_commod>bears</growth_commod>
          <growth_rate>0.05</growth_rate>
          <prototypes><val>BearStore</val></prototypes>
          <initial_demand>20.0</initial_demand>
        </NOInst>
      </config>
      <initialfacilitylist>
        <entry>
          <number>2</number>
          <prototype>BearStore</prototype>
        </entry>
      </initialfacilitylist>
      <name>SingleInstitution</name>
    </institution>
    <name>SingleRegion</name>
  </region>
</simulation>
