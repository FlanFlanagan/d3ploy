<simulation>
  <archetypes>
    <spec>
      <lib>agents</lib>
      <name>NullRegion</name>
    </spec>
    <spec>
      <lib>cycamore</lib>
      <name>Source</name>
    </spec>
    <spec>
      <lib>d3ploy.no_inst</lib>
      <name>NOInst</name>
    </spec>
  </archetypes>
  <control>
    <duration>3</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>
  <facility>
    <config>
      <Source>
        <outcommod>fuel</outcommod>
        <outrecipe>fuel</outrecipe>
        <throughput>1</throughput>
      </Source>
    </config>
    <name>source</name>
  </facility>
  <recipe>
    <basis>mass</basis>
    <name>fuel</name>
    <nuclide>
      <comp>0.711</comp>
      <id>U235</id>
    </nuclide>
    <nuclide>
      <comp>99.289</comp>
      <id>U238</id>
    </nuclide>
  </recipe>
  <region>
    <config>
      <NullRegion>
      </NullRegion>
    </config>
    <institution>
      <config>
        <NOInst>
          <calc_method>arma</calc_method>
          <demand_commod>fuel_end</demand_commod>
          <demand_std_dev>0.0</demand_std_dev>
          <growth_rate>0.0</growth_rate>
          <initial_demand>1</initial_demand>
          <prototypes>
            <val>source</val>
          </prototypes>
          <steps>1</steps>
          <supply_commod>fuel</supply_commod>
        </NOInst>
      </config>
      <name>source_inst</name>
    </institution>
    <name>SingleRegion</name>
  </region>
</simulation>
